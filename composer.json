{
	"name": "soderlind/mediamanager",
	"description": "Virtual folder organization and smart management for the WordPress Media Library.",
	"type": "wordpress-plugin",
	"require": {
		"php": ">=8.3",
		"yahnis-elsts/plugin-update-checker": "^5.6"
	},
	"require-dev": {
		"phpunit/phpunit": "^11.0",
		"brain/monkey": "^2.6"
	},
	"autoload": {
		"classmap": [
			"includes/"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"MediaManagerTests\\": "tests/php/"
		}
	},
	"scripts": {
		"test": "phpunit"
	}
}